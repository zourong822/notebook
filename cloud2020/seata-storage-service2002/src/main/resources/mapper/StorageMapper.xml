<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.springcloud.dao.StorageMapper">
    <update id="storageChange">
        UPDATE t_storage
        SET used = used + #{count},residue=residue-#{count} WHERE product_id=#{productId}
    </update>
</mapper>